{
    "name": "Dart Development Environment",
    "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
    "customizations": {
      "vscode": {
        "extensions": [
          "Dart-Code.dart-code",
          "Dart-Code.flutter"
        ]
      }
    },
    "forwardPorts": [8080],
    "postCreateCommand": "dart --version"
  }